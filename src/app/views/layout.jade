doctype html
html(lang="es")
	head
		title CRUD Usuarios
		meta(charset="utf-8")

		<!-- Archivos Css -->

		<!--  Bootstrap CSS -->
		link(href="../../css/bootstrap.min.css" rel="stylesheet")

		<!--  Animaciones para Notificaciones -->
		link(href="../../css/animate.min.css" rel="stylesheet")

		<!-- Bootstrap Panel -->
		link(href="../../css/light-bootstrap-dashboard.css" rel="stylesheet")

		<!-- Archivo -->
		link(href="../../css/demo.css" rel="stylesheet")

		<!-- Fonts and icons -->
		<link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
		<link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
		link(href="../../css/pe-icon-7-stroke.css" rel="stylesheet")
		
	body
		div(class="wrapper")
			div(class="sidebar" data-color="blue" data-image="../../img/panel/sidebar-1.jpg")

				div(class="sidebar-wrapper")
					div(class="logo")
						a(href="#/!" class="simple-text") CRUD Usuarios

					ul(class="nav")
						block menu

			div(class="main-panel")

				nav(class="navbar navbar-default navbar-fixed")

					div(class="container-fluid")
						div(class="navbar-header" style="height:65px")
							button(type="button" class="navbar-toggle" data-toggle="collapse")
								span(class="sr-only") Toggle navigation
								span(class="icon-bar")
								span(class="icon-bar")
								span(class="icon-bar")

				div(class="content")
					div(class="container-fluid")
						div(class="row")
							div(class="col-md-12")
								div(class="card")
									div(class="content")
										block contenido
										div(class="clearfix")

				footer(class="footer")
					div(class="container-fluid")
						p(class="copyright pull-right") &copy;CRUD De Usuarios

	<!--   Core JS Files   -->
	script(src="../../js/jquery-1.10.2.js" type="text/javascript")
	script(src="../../js/bootstrap.min.js" type="text/javascript")

	script(src="../../js/bootstrap-checkbox-radio-switch.js" type="text/javascript")

	script(src="../../js/chartist.min.js" type="text/javascript")

	script(src="../../js/bootstrap-notify.js" type="text/javascript")

	script(src="../../js/light-bootstrap-dashboard.js" type="text/javascript")

	script(src="../../js/demo.js" type="text/javascript")

